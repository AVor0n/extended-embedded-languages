{
  "name": "extended-embedded-languages",
  "displayName": "Extended Embedded Languages",
  "publisher": "ruschaaf",
  "icon": "images/EEL_icon_256.png",
  "description": "Adds syntax highlighting within multiline strings, for C++, Python, Javascript and YAML",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "syntax highlighting",
    "nested",
    "comments",
    "embedded",
    "language"
  ],
  "author": {
    "name": "ruschaaf",
    "email": "russ.schaaf@gmail.com",
    "url": "https://github.com/ruschaaf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruschaaf/extended-embedded-languages"
  },
  "homepage": "https://github.com/ruschaaf/extended-embedded-languages",
  "contributes": {
    "grammars": [
      {
        "scopeName": "source.cpp.embedded.codeblock",
        "injectTo": [
          "source.cpp"
        ],
        "path": "syntaxes/cpp.embedded.json",
        "embeddedLanguages": {
          "meta.embedded.block.bat.cpp": "bat",
          "meta.embedded.block.cpp.cpp": "cpp",
          "meta.embedded.block.css.cpp": "css",
          "meta.embedded.block.html.cpp": "html",
          "meta.embedded.block.ini.cpp": "ini",
          "meta.embedded.block.javascript.cpp": "javascript",
          "meta.embedded.block.json.cpp": "json",
          "meta.embedded.block.jsonc.cpp": "jsonc",
          "meta.embedded.block.python.cpp": "python",
          "meta.embedded.block.shell.cpp": "shell",
          "meta.embedded.block.sql.cpp": "sql",
          "meta.embedded.block.toml.cpp": "toml",
          "meta.embedded.block.xml.cpp": "xml",
          "meta.embedded.block.yaml.cpp": "yaml",
          "meta.embedded.block.c.cpp": "c",
          "meta.embedded.block.hlsl.cpp": "hlsl",
          "meta.embedded.block.glsl.cpp": "glsl",
          "meta.embedded.block.metal.cpp": "metal",
          "meta.embedded.block.wgsl.cpp": "wgsl",
          "meta.embedded.block.lua.cpp": "lua",
          "meta.embedded.block.makefile.cpp": "makefile",
          "meta.embedded.block.graphql.cpp": "graphql",
