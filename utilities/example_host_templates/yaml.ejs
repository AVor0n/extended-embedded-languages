doc_examples:

  test_code: | #cpp
    int main() { return 0; }

  db_insert_cmd: | #sql
    INSERT INTO users
    VALUES (1, "foo", 3);


######################################################################
## Embedded Examples:

embedded_examples:
# There may be another way to signal which language to use in yaml,
# but for now we will use a comment after the block marker
<% snippets.forEach(function(snippet) { %>
  # <%= snippet.name %> Example
  sample_<%= snippet.ids[0] %>: | #<%-snippet.ids[0]%>
    <%-snippet.code.replaceAll("\n", "\n    ")%>
<% }); %>


